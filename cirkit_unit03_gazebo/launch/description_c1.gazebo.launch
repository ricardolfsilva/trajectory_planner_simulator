  <remap from="/gains/base_to_left_rear_wheel/parameter_updates" to="/gains/base_to_left_rear_wheel/parameter_updates_c1"/>
  <remap from="/gains/base_to_right_rear_wheel/parameter_descriptions" to="/gains/base_to_right_rear_wheel/parameter_descriptions_c1"/>
  <remap from="/gains/base_to_right_rear_wheel/parameter_updates" to="/gains/base_to_right_rear_wheel/parameter_updates_c1"/>
  <param name="robot_description"
         command="$(find xacro)/xacro --inorder '$(find cirkit_unit03_gazebo)/urdf/description_c1.gazebo.xacro'" />
