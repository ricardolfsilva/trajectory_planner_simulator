<?xml version="1.0"?>
<!-- ackermann_vehicle.launch

Use Gazebo to simulate a vehicle with Ackermann steering. -->
<launch>
  <remap from="/reduced_pcl" to="/reduced_pcl_c2"/>
  <remap from="/simulator_laser_scan" to="/simulator_laser_scan_c2"/>
  <remap from="/steer_drive_controller/cmd_vel" to="/steer_drive_controller/cmd_vel_c2"/>
  <remap from="/steer_drive_controller/odom" to="/steer_drive_controller/odom_c2"/>
  
  <!-- Load the URDF for GAZEBO into the ROS Parameter Server -->
  <include file="$(find cirkit_unit03_gazebo)/launch/cirkit_unit03_spawn_c2.launch"/>
  <node name="cirkit_unit03_gazebo_c2" pkg="cirkit_unit03_gazebo" type="cirkit_unit03_gazebo_node" output="screen"/>
  <!-- Trajectory planning -->
  <include file="$(find trajectory_planner)/launch/trajectory_planner_simulated_simulator_c2.launch"/>
  <!-- <node name="tf_generator4_c2" pkg="cirkit_unit03_gazebo" type="tf_generator4" />
  <node name="tf_generator5_c2" pkg="cirkit_unit03_gazebo" type="tf_generator5" /> -->
</launch>
